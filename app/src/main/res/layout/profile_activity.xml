<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/editProfileButton"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/profile_gradiant">


    <com.google.android.material.button.MaterialButton
        android:id="@+id/logout_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="77dp"
        android:layout_marginBottom="@dimen/bottom_margin"
        android:background="#00FFFFFF"
        android:fontFamily="@font/gilroybold"
        android:text="@string/log_out"
        android:textSize="@dimen/t1_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/delete_account_button"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/bottom_margin"
        android:background="#00FFFFFF"
        android:backgroundTint="#00FFFFFF"
        android:fontFamily="@font/gilroylight"
        android:text="@string/delete_account"
        android:textSize="@dimen/t1_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="318dp"
        android:layout_height="match_parent"
        android:layout_marginStart="54dp"
        android:layout_marginTop="300dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:srcCompat="@drawable/purpule_bottom_profile" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/youhave_textView"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="150dp"
        android:fontFamily="@font/gilroybold"
        android:text="@string/you_have"
        android:textColor="#292D30"
        android:textSize="@dimen/h1_size"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/youhave_textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/side_margin"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/gilroybold"
        android:text="@string/created"
        android:textColor="@color/white"
        android:textSize="@dimen/t1_size"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/youhave_textView" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/youhave_textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/side_margin"
        android:fontFamily="@font/gilroybold"
        android:text="@string/events"
        android:textColor="@color/white"
        android:textSize="@dimen/t1_size"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/youhave_textView5" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/youhave_textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/side_margin"
        android:fontFamily="@font/gilroybold"
        android:text="@string/events"
        android:textColor="@color/white"
        android:textSize="@dimen/t1_size"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/youhave_textView7" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/youhave_textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:fontFamily="@font/gilroybold"
        android:text="@string/friends"
        android:textColor="@color/white"
        android:textSize="@dimen/t1_size"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/youhave_textView9" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/youhave_textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/side_margin"
        android:fontFamily="@font/gilroybold"
        android:text="@string/test_num"
        android:textColor="@color/white"
        android:textSize="@dimen/h1super_num_size"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/youhave_textView2" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/youhave_textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:fontFamily="@font/gilroybold"
        android:text="@string/test_num"
        android:textColor="@color/white"
        android:textSize="@dimen/h1super_num_size"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/youhave_textView4" />

    <TextView
        android:id="@+id/youhave_textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:fontFamily="@font/gilroybold"
        android:text="@string/test_num"
        android:textColor="@color/white"
        android:textSize="@dimen/h1super_num_size"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/youhave_textView3" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/youhave_textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/gilroybold"
        android:text="@string/connected"
        android:textColor="@color/white"
        android:textSize="@dimen/t1_size"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/youhave_textView8" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/youhave_textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/side_margin"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/gilroybold"
        android:text="@string/attended"
        android:textColor="@color/white"
        android:textSize="@dimen/t1_size"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/youhave_textView6" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="318dp"
        android:layout_height="0dp"
        android:adjustViewBounds="true"
        android:cropToPadding="true"
        android:hapticFeedbackEnabled="false"
        android:scaleType="fitStart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/youhave_textView3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.49"
        app:srcCompat="@drawable/profile_background" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="0dp"
        android:layout_height="350dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:srcCompat="@drawable/white_box" />


    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/profileImage"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="10dp"
        android:src="@drawable/signup_upload_picture_icon"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView8" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/edit_profile_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/side_margin"
        android:layout_marginTop="@dimen/side_margin"
        android:background="@null"
        android:fallbackLineSpacing="false"
        android:fontFamily="@font/gilroybold"
        android:singleLine="false"
        android:text="@string/terry_meyer"
        android:textColor="@color/black"
        android:textSize="@dimen/h2_size"
        app:layout_constraintStart_toEndOf="@+id/profileImage"
        app:layout_constraintTop_toTopOf="@+id/imageView8" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/side_margin"
        android:layout_marginTop="@dimen/side_margin"
        android:fontFamily="@font/gilroybold"
        android:text="@string/email"
        android:textColor="@color/black"
        android:textSize="@dimen/h2_size"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/profileImage" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/side_margin"
        android:layout_marginTop="@dimen/side_margin"
        android:fontFamily="@font/gilroybold"
        android:text="@string/password"
        android:textColor="@color/black"
        android:textSize="@dimen/h2_size"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/profile_email"/>

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/progile_username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/side_margin"
        android:fontFamily="@font/gilroybold"
        android:text="@string/username_tm"
        android:textSize="@dimen/t1_size"
        app:layout_constraintStart_toEndOf="@+id/profileImage"
        app:layout_constraintTop_toBottomOf="@+id/edit_profile_name"
        android:background="@null"/>

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/profile_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:fontFamily="@font/gilroybold"
        android:text="@string/username_tm"
        android:textSize="@dimen/t1_size"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/email"
        android:background="@null"/>

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/profile_password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/side_margin"
        android:fontFamily="@font/gilroybold"
        android:text="@string/username_tm"
        android:textSize="@dimen/t1_size"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/password"
        android:background="@null"/>

    <com.getbase.floatingactionbutton.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:paddingBottom="15dp"
        android:clickable="true"
        app:fab_colorNormal="@color/purple_light"
        app:fab_colorPressed="@color/purple_light"
        app:fabSize="auto"
        app:layout_constraintBottom_toBottomOf="@+id/imageView8"
        app:layout_constraintEnd_toEndOf="@+id/imageView8"
        android:src="@drawable/edit_icon"
        android:id="@+id/editProfileInfoButton"/>


</androidx.constraintlayout.widget.ConstraintLayout>